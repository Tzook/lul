{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "allowJs": true,
    "outDir": "output"
  },
  "exclude": [
    "gulpfile.js",
    "node_modules",
    "output",
    "typings"
  ],
  "compileOnSave": false
}